package ex0624;

public class DD {

	public static void main(String[] args) {
		
		final int AA=10;//final에는 값을 한번만 넣을수있음 바꿀수X
		final String CC="문자열";
		
		System.out.println("AA="+AA);
		System.out.println("CC="+CC);
		
		System.out.println("안녕하세요\n");
		System.out.println("안녕하세요\t박영회입니다.");
		System.out.println("안녕하세요");
		System.out.println("\'안녕하세요\'");
		System.out.println("\\안녕하세요\\");
		//백슬래시 이용 외우기(이스케이프시퀀스)
	}
	
}
