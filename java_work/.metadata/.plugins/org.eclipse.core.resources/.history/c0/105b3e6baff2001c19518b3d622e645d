
public class FF {

	public static void main(String[] args) {
		
		boolean a=true;
		boolean aa=false;// 참이나거짓넣을때 명령어임
		System.out.println(a);
		
		char b='A';// char은한문자만넣을수있음->쌍따옴표아님
		int c=123123123;
		
		float kk=1.2f;//f는float이라서그냥적은겨
		double qq=1.3d;//d는double이라서그냥적은겨
		
		System.out.print(kk);
		System.out.print(dd);
		
	}
}
